          "settings": {
            "custom_liquid": "<style>\n  .tessi-waitlist {\n    text-align: center;\n    max-width: 800px;\n    margin: 0 auto;\n    display: flex;\n    flex-direction: column;\n    min-height: 85vh; /* INCREASED to push form down */\n    justify-content: space-between;\n  }\n  \n  .tessi-waitlist__top {\n    padding-top: 50px;\n  }\n  \n  /* SVG LOGO STYLING - NEW */\n  .tessi-waitlist__logo-image {\n    width: 650px; /* SMALLER LOGO */\n    height: auto;\n    margin-bottom: 20px; /* MORE SPACING */\n    display: block;\n    margin-left: auto;\n    margin-right: auto; /* ENSURES CENTERING */\n  }\n  \n  .tessi-waitlist__bottom {\n    padding-bottom: 60px;\n  }\n  \n  .tessi-waitlist__logo {\n    font-size: 4rem;\n    font-weight: 300;\n    letter-spacing: 0.5em;\n    color: white;\n    margin-bottom: 10px;\n    line-height: 1;\n  }\n  \n  .tessi-waitlist__tagline {\n    font-size: 1.25rem;\n    letter-spacing: 0.5em;\n    color: rgba(255,255,255,0.8);\n    margin-bottom: 0;\npadding-left: 3.5rem\n  }\n  \n  .tessi-waitlist__heading {\n    font-size: 0.9rem;\n    font-weight: 600;\n    letter-spacing: 0.2em;\n    color: white;\n    margin-bottom: 8px;\n    line-height: 1.5;\n  }\n  \n  .tessi-waitlist__subheading {\n    font-size: 0.75rem;\n    font-weight: 400;\n    letter-spacing: 0.2em;\n    color: rgba(255,255,255,0.9);\n    margin-bottom: 30px;\n    line-height: 1.5;\n  }\n  \n  .tessi-waitlist__form {\n    display: flex;\n    gap: 12px;\n    align-items: stretch;\n  }\n  \n  .tessi-waitlist__form input {\n    flex: 1;\n    padding: 16px 24px;\n    margin-bottom: 0;\n    background: transparent;\n    border: 1px solid rgba(255,255,255,0.4);\n    color: white;\n    font-size: 0.85rem;\n    letter-spacing: 0.15em;\n    text-align: center;\n    transition: all 0.3s ease;\n  }\n  \n  .tessi-waitlist__form input::placeholder {\n    color: rgba(255,255,255,0.5);\n    letter-spacing: 0.15em;\n  }\n  \n  .tessi-waitlist__form input:focus {\n    outline: none;\n    border-color: rgba(255,255,255,0.8);\n    background: rgba(255,255,255,0.05);\n  }\n  \n  .tessi-waitlist__form button {\n    flex: 0 0 auto;\n    min-width: 200px;\n    padding: 16px 24px;\n    background: white;\n    color: black;\n    border: none;\n    font-weight: 600;\n    letter-spacing: 0.15em;\n    cursor: pointer;\n    transition: all 0.3s ease;\n    font-size: 0.85rem;\n    white-space: nowrap;\n  }\n  \n  .tessi-waitlist__form button:hover {\n    background: rgba(255,255,255,0.9);\n    transform: translateY(-2px);\n  }\n  \n  .tessi-waitlist__success {\n    color: white;\n    font-size: 1rem;\n    letter-spacing: 0.1em;\n    padding: 20px;\n    background: rgba(255,255,255,0.1);\n    border: 1px solid rgba(255,255,255,0.3);\n    margin-top: 20px;\n  }\n  \n  /* Mobile responsive */\n  @media (max-width: 768px) {\n    .tessi-waitlist {\n      max-width: 500px;\n      min-height: 75vh; /* Adjusted for mobile */\n    }\n    \n    .tessi-waitlist__logo-image {\n      width: 350px; /* Smaller on mobile */\n      margin-bottom: 20px;\n    }\n    \n    .tessi-waitlist__logo {\n      font-size: 2.5rem;\n      letter-spacing: 0.3em;\n    }\n    \n    .tessi-waitlist__tagline {\n      font-size: 0.7rem;\n      padding-left: 0rem\n    }\n    \n    .tessi-waitlist__heading {\n      font-size: 0.8rem;\n    }\n    \n    .tessi-waitlist__subheading {\n      font-size: 0.7rem;\n    }\n    \n    .tessi-waitlist__form {\n      flex-direction: column;\n      gap: 15px;\n    }\n    \n    .tessi-waitlist__form button {\n      min-width: 100%;\n    }\n  }\n</style>\n\n<div class=\"tessi-waitlist\">\n  <!-- TOP SECTION (Logo & Tagline) -->\n  <div class=\"tessi-waitlist__top\">\n<img \n  src=\"{{ 'Tessi_logo_white.svg' | asset_url }}\" \n  alt=\"tessi\" \n  class=\"tessi-waitlist__logo-image\"\n/>\n    <p class=\"tessi-waitlist__tagline\">WHERE STYLE SERVES</p>\n  </div>\n  \n  <!-- BOTTOM SECTION (Sign up form) -->\n  <div class=\"tessi-waitlist__bottom\">\n    {% if form.posted_successfully? %}\n      <div class=\"tessi-waitlist__success\">\n        âœ“ YOU'RE ON THE LIST\n      </div>\n    {% else %}\n      <p class=\"tessi-waitlist__heading\">\n        SIGN UP TO KNOW WHEN WE LAUNCH\n      </p>\n      <p class=\"tessi-waitlist__subheading\">\n        EARLY ACCESS TO THE FIRST DROP\n      </p>\n      \n      {% form 'contact', id: 'tessi-waitlist-form', class: 'tessi-waitlist__form' %}\n        {% if form.errors %}\n          <div class=\"tessi-waitlist__success\" style=\"background: rgba(255,0,0,0.1); border-color: rgba(255,0,0,0.3);\">\n            Please check your information\n          </div>\n        {% endif %}\n        \n        <input type=\"hidden\" name=\"contact[tags]\" value=\"waitlist\" />\n        \n        <input \n          type=\"text\" \n          name=\"contact[first_name]\" \n          placeholder=\"NAME\" \n          required \n        />\n        \n        <input \n          type=\"email\" \n          name=\"contact[email]\" \n          placeholder=\"EMAIL\" \n          required \n        />\n        \n        <button type=\"submit\">JOIN WAITLIST</button>\n      {% endform %}\n    {% endif %}\n  </div>\n</div>"
          },
